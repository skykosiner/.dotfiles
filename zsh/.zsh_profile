VIM="nvim"
export YONIKOSINER=$HOME/personal/yonikosiner-com.git
export TODO_ME_DADDY=$HOME/personal/todo-me-daddy

export EDITOR="nvim"

PERSONAL=$XDG_CONFIG_HOME/personal

#!/bin/bash

while [ "$#" -gt 0 ]; do
    curr=$1
    shift

    case "$curr" in
    "-gc")
        open https://classroom.google.com/u/3/h
        ;;
    "-ns")
        open https://www.notion.so/kosiner/School-ebdb1a7a8e3f466aa8d4400ffd45fbb0
        ;;
    *) echo "Unavailable command... $curr"
    esac
done

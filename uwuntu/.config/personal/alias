alias ls="ls -A | lolcat"

alias c="clear"
alias ll="ls -lh"
alias ..="cd .."
alias keyboard="setxkbmap -layout real-prog-dvorak"

alias :q="exit"

alias mv='mv -i'
alias rm='rm -i'

alias vim="nvim"

alias evrc="vim ~/.dotfiles/nvim/.config/nvim"

alias sync_mail="mw -Y"

alias math="expr"

alias dvorakBTW="cowsay 'I Use Dvorak Btw' | lolcat"

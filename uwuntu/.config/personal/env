change_background() {
    feh --bg-scale $HOME/.dotfiles/anime/$(ls ~/.dotfiles/anime | fzf)
}

commitDotFiles() {
    pushd $DOTFILES
    git add .
    git commit -m "automagic messaging from me in the past.  Have you checked up your butthole?"
    git push origin master
    popd
}

snyc_mail() {
    mw -y
}

commitSchoolWork() {
    pushd $SCHOOLWORK
    git add .
    git commit -m "automagic messaging from me in the past.  Have you checked up your butthole?"
    git push origin master
    popd
}

#!/usr/bin/env bash

configs="dwm | ~/personal/suckless/dwm ! zsh | ~/.dotfiles/zsh ! moreZsh | ~/.dotfiles/shell/.config/personal ! scripts | ~/.dotfiles/bin/.local/bin ! lf | ~/.dotfiles/lf/.config/lf ! nvim | ~/.dotfiles/nvim/.config/nvim/"
config=$(echo "$configs" | tr '!' '\n' | dmenu)
st -e $EDITOR $(echo $config | awk -F '|' '{print $2}')

#!/usr/bin/env bash

configs="emacs | ~/.dotfiles/doom-emacs/.doom.d/ ! dwm | ~/personal/suckless/dwm ! zsh | ~/.dotfiles/zsh ! moreZsh | ~/.dotfiles/DvoarkBTW/.config/personal ! scripts | ~/.dotfiles/bin/.local/bin ! lf | ~/.dotfiles/lf/.config/lf ! "
config=$(echo "$configs" | tr '!' '\n' | dmenu)
$EDITOR $(echo $config | awk -F '|' '{print $2}')

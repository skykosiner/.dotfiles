    {/home/sky/.local/bin/lights ceiling, /home/sky/.local/bin/lights studio, /home/sky/.local/bin/lights others, /home/sky/.local/bin/lights al, /home/sky/.local/bin/light-percent, /home/sky/.local/bin/light-temp}


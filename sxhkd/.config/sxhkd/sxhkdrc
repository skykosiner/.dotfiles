    {/home/sky/.local/bin/aircon toggle, /home/sky/.local/bin/cold, /home/sky/.local/bin/hot, /home/sky/.local/bin/airconControl -change, /home/sky/.local/bin/airconControl -sellect-fan}

super + s ; {p, period, comma, shift + p, a, s, m, l}
    {/home/sky/.local/bin/spotify-control -togglePlay, /home/sky/.local/bin/spotify-control -next, /home/sky/.local/bin/spotify-control -prev, /home/sky/.local/bin/spotify-control -play-song, /home/sky/.local/bin/spotify-control -select-album, /home/sky/.local/bin/spotify-control -select-playlist, wezterm -e spt, wezterm -e sptlrx}

